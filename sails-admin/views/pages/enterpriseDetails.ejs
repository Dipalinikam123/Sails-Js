<!DOCTYPE html>
<html>

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link href="" rel="stylesheet" />
  <title></title>
  <style>
    .main-div{
      background-color: #f4f4f4;
    }
    .table-div {
      font-family: Arial, sans-serif;
    
      margin: 0;
      padding: 20px;
      display: flex;
      justify-content: center;
      align-items: center;
      height: 100vh;
    }

    table {
      width: 80%;
      border-collapse: collapse;
      margin: 20px auto;
      background-color: #fff;
      box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);
    }

    th,
    td {
      padding: 12px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }

    th {
      background-color: #4CAF50;
      color: white;
      font-weight: bold;
    }

    tr:nth-child(even) {
      background-color: #f2f2f2;
    }

    tr:hover {
      background-color: #e0e0e0;
    }

    th,
    td {
      text-align: center;
    }

    .logo {
      max-width: 100px;
      max-height: 50px;
    }
    button{
      padding: 10px 15px;
      background-color: #4CAF50;
      color: white;
      border-radius: 5px;
    }
  </style>
</head>

<body>
  <%= console.log("---enterpriseId",enterpriseUsers) %>
    <div class="main-div">
      <button onclick="window.location.href='/register'">Add User</button>
      <div class="table-div">
        <table>
          <thead>
            <tr>
              <th>Name</th>
              <th>Email</th>
              <th>Role</th>
              <th>enterpriseId</th>


            </tr>
          </thead>
          <tbody>
            <% enterpriseUsers.map(e=> { %>
              <tr>
                <td>
                  <%= e.name %>
                </td>
                <td>
                  <%= e.email %>
                </td>
                <td>
                  <%= e.role %>
                </td>
                <td>
                  <%= e.enterpriseId %>
                </td>
              </tr>
              <% }) %>
          </tbody>
        </table>
      </div>
    </div>
</body>

</html>